{
  "name": "@prantlf/gen-readlines",
  "version": "1.0.0",
  "description": "Generator based line reader",
  "engines": {
    "node": ">=6"
  },
  "main": "index",
  "scripts": {
    "test": "nyc mocha -R spec tests.js tests-simplified.js",
    "lint": "eslint *.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/gen-readlines.git"
  },
  "keywords": [
    "generator",
    "file",
    "line",
    "reader",
    "read",
    "by",
    "es6",
    "ecma2015"
  ],
  "author": "Eric Bower",
  "contributors": [
    "Ferdinand Prantl <prantlf@gmail.com> (http://prantl.tk)"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/gen-readlines/blob/master/LICENSE.md"
    }
  ],
  "bugs": {
    "url": "https://github.com/prantlf/gen-readlines/issues"
  },
  "homepage": "https://github.com/prantlf/gen-readlines#readme",
  "nyc": {
    "include": "index.js",
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "checkCoverage": true,
    "branches": 100,
    "statements": 100,
    "functions": 100,
    "lines": 100
  },
  "devDependencies": {
    "eslint": "7.18.0",
    "mocha": "8.2.1",
    "nyc": "15.1.0"
  }
}
